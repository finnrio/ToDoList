<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="400.0" prefWidth="430.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ListRemoveController" fx:id="removePane">
   <center>

      <ChoiceBox fx:id="listChoiceBox" prefWidth="300" />
   </center>
   <bottom>
      <Button fx:id="removeBtn" onAction="#handleRemoveBtnAction" text="Remove">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" />
         </BorderPane.margin></Button>
   </bottom>
</BorderPane>
